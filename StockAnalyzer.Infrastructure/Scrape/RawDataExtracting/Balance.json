{
  "periods": {
    "_xpath": "//th[contains(@class,'thq')]/text()[1]",
    "_transformations": [
      "TrimTransformation"
    ]
  },
  "rows": {
    "_xpath": "//tr[contains(@data-field,'Balance')]",
    "_forceArray": true,
    "label": {
      "_xpath": ".//@data-field",
      "_transformations": [
        {
          "_type": "RegexValueRemoveTransform",
          "_regex": "[A-Z][a-z]*",
          "_maxCount": "1"
        }
      ]
    },
    "description": ".//td[@class='f']//a",
    "vals": {
      "_xpath": ".//td[contains(@class, 'h')]//span[@class='value']//span//span",
      "_forceArray": true,
      "_transformations": [
        {
          "_type": "RegexValueRemoveTransform",
          "_regex": "\\s"
        }
      ]
    }
  }

}
